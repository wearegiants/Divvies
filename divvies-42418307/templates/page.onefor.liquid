<div class="fs-row">
  <div class="fs-cell fs-all-full">
    <section id="page" class="oneforme">
      <h1 class="header--title"><img alt="One For Me, One For You" src="{{ 'Divvies_CookieDisplayShelvingStickers_OUTPUT_02v3.jpg' | asset_url }}" class="img-responsive" /></h1>
      <div class="fs-row">

        {% unless page.handle contains 'thank-you' %}

        <div class="fs-cell fs-lg-7 fs-md-3 fs-sm-3"><div class="wrapper oneforme__content">{{ page.content }}</div></div>      
        <div class="fs-cell fs-lg-5 fs-md-3 fs-sm-3"><div class="wrapper oneforme__form">{% include 'mailchimp-form-onefor' %}</div></div>      

        {% else %}

        <div class="fs-cell fs-lg-7 fs-md-3 fs-sm-3 fs-centered"><div class="wrapper oneforme__content text-center">{{ page.content }}</div></div>      

        {% endunless %}
      </div>
    </section>
  </div>
</div>

<script>
  $(document).ready(function(){
    fbq('track', 'ViewContent');
  });
</script>