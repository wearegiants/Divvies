<section id="page" class="page--about">

   <header id="page--header" class="header">
      <div class="fs-row">
         <div class="fs-cell fs-all-full">
            <h1 class="header--title">{{ page.title }}</h1>
         </div>
      </div>
   </header>

   <div class="page--content">
      <div class="fs-row">
         <div class="fs-cell fs-all-full">
            {{ page.content }}
         </div>
      </div>
   </div>

   <hr class="invisible">

   <div class="quote">
      <div class="fs-row">
         <div class="fs-cell fs-all-full">
            {{ pages.about-page-quote.content }}
         </div>
      </div>
   </div>

</section>
